chip soc/mediatek/mt8173
	device cpu_cluster 0 on
		device cpu 0 on end
	end
	register "framebuffer_bits_per_pixel" = "16"
	register "pixelclock" = "76000"
	register "hactive" = "1368"
	register "hfront_porch" = "100"
	register "hback_porch" = "106"
	register "hsync_len" = "26"
	register "vactive" = "768"
	register "vfront_porch" = "10"
	register "vback_porch" = "10"
	register "vsync_len" = "12"
end
