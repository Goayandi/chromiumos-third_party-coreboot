	bool
	default n
	select ARCH_BOOTBLOCK_ARM_V4
	select ARCH_ROMSTAGE_ARM_V8_64
	select ARCH_RAMSTAGE_ARM_V8_64
	select ARM_LPAE

if SOC_NVIDIA_TEGRA132

